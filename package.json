{
  "name": "gulp-etl-tap-csv",
  "version": "1.0.8",
  "repository": "https://github.com/gulpetl/gulp-etl-tap-csv",
  "description": "Convert CSV files to JSON compatible with gulp-etl",
  "main": "dist/js/plugin.js",
  "typings": "dist/types/plugin.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && tsc --module commonjs --outDir dist/lib",
    "prebuild": "pnpm run clean",
    "clean": "rimraf dist/*",
    "test": "jest",
    "bump": "pnpm version patch && git push --tags origin master"
  },
  "author": "peterl@sbcotton.com",
  "license": "MIT",
  "keywords": [
    "gulpplugin",
    "csv",
    "gulp-etl",
    "data"
  ],
  "node-red": {
    "nodes": {
      "gulpetl-tap-csv": "gulpetl-tap-csv.js"
    }
  },
  "homepage": "http://docs.gulpetl.com",
  "dependencies": {
    "csv-parse": "^5.5.2",
    "from2": "^2.3.0",
    "gulp-error-handle": "^1.0.1",
    "loglevel": "^1.8.1",
    "merge": "^2.1.1",
    "pkginfo": "^0.4.1",
    "plugin-error": "^1.0.1",
    "replace-ext": "^2.0.0",
    "stream-transform": "^3.2.10",
    "through2": "^3.0.2",
    "vinyl": "^2.2.1"
  },
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/node": "^10.17.60",
    "@types/replace-ext": "^2.0.2",
    "@types/vinyl": "^2.0.11",
    "@types/vinyl-paths": "0.0.31",
    "gulp": "^4.0.2",
    "jest": "^27.5.1",
    "jest-spec-reporter": "^1.0.19",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "pnpm": {
    "overrides": {
      "glob-parent@<5.1.2": ">=5.1.2"
    }
  }
}
